<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Quickabdest" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="android:windowNoTitle">true</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/main_mid</item>
        <item name="colorPrimaryVariant">@color/main_dark</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#FFFFFFFF</item>
        <item name="android:navigationBarColor" tools:targetApi="l">#FFFFFFFF</item>
        <!-- Customize your theme here. -->
    </style>
</resources>